{
  "name": "jobs-app",
  "private": true,
  "scripts": {
    "dev": "concurrently  \"next dev\" \"cd server\" \"npm run start\"",
    "build": "next build",
    "start": "concurrently \"next start\" \"cd server && npm run start\"",
    "lint": "eslint --ext .ts,.tsx ./src --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --ext .ts,.tsx ./src --no-error-on-unmatched-pattern --fix",
    "prettier": "prettier \"**/*.+(json|ts|tsx)\"",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "types:check": "tsc --noEmit --pretty",
    "prepare": "husky install",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
    "stage:switch": "node scripts/switch-stage.js",
    "stage:save": "node scripts/save-stage.js",
    "stage:clear": "node scripts/clear-stage.js"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@glidejs/glide": "^3.6.0",
    "@icon-park/react": "^1.4.2",
    "@preline/overlay": "^1.4.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/react-query": "^4.2.3",
    "@tanstack/react-query-devtools": "^4.2.3",
    "abort-controller": "^3.0.0",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "framer-motion": "^7.2.1",
    "next": "^13.2.4",
    "next-redux-wrapper": "^8.1.0",
    "node-fetch": "^2.6.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.34.2",
    "react-redux": "^7.2.9",
    "redux-persist": "^6.0.0",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@mswjs/data": "^0.10.2",
    "@testing-library/react": "^13.3.0",
    "@types/glidejs__glide": "^3.6.1",
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.6.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.4",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "jest-environment-jsdom": "^29.0.2",
    "lint-staged": "^13.0.3",
    "msw": "^0.47.4",
    "msw-devtools": "^1.0.2",
    "postcss-import": "^15.1.0",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.7",
    "ts-eager": "^2.0.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "4.8.2",
    "webpack": "^5.74.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
